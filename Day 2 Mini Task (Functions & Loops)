// To find the highest marks from an array.

function findHighestMarks (marks) {
 if (marks.length == 0) {
   return null;
}

let highestMarks = marks[0];

for (let i = 1; i < marks.length; i++) {

      if (marks[i] > highestMarks) {
         highestMarks = marks[i];
      }
}

return highestMarks;
}


const studentMarks = [45, 53, 29, 89, 93];
const highest = findHighestMarks(studentMarks);

console.log("The highest mark is: " + highest);

// Output : The highest mark is: 93
